////// plugin1/live.package.json

{
    "name" : "plugin1",
    "version" : "1.0.0"
}


////// main.lv.js

imports.require(".plugin1.subplugin2")

var C = imports.get("C")

module.exports["main"] = class extends Element{
    constructor(){
        super()
        var c = new C();
        Element.addProperty(this, "c", {
            type: "string",
            value: c.toString(),
            notify: "cChanged"
        })
    }
}
